@startuml

class GameLoop {
  -<List> Element
  +render(Element)
  +userInput()
  +run()
}

class Render {
  +render()
}

abstract class Bullet {
  +update()
  +render()
}

abstract class Element {
  -x,
  -y,
  -vx,
  -vy,
  +update()
  +render()
}

class Ball {
  -radius
}

class Cannon {
  -angle
  -power
}

 class  Gravity {
  -{static} gravityVal
  +{static} updateMovement()
}

class Slider {
  -min
  -max
  -width
  -height
  -lbel
  -sliderX
  -sliderWidth
  -sliderHeight
  -sliderGrapped
  +render()
  +getVal()
}

Bullet <|-- Ball
GameLoop *-- Render
GameLoop *-- Element
Element *-- Bullet
Element *-- Slider
Element *-- Cannon

@enduml
