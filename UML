@startuml

class GameLoop {
  -<Vector> Bullet
  -Cannon
  +main()
  +userInput()
  +run()
}

abstract class Bullet {
  -x,
  -y,
  -vx,
  -vy,
  -ax,
  -ay,
  +update()
  +render()
}

class Ball {
  -radius
}

class Cannon {
  -angle
  -power
}

class Gravity {
  -gravityVal
  +updateMovement()
}

Bullet <|-- Ball
GameLoop *-- Bullet
GameLoop *-- Cannon

@enduml
